{
	parser: 'babel-eslint',
	parserOptions: {
		ecmaVersion: 6,
		sourceType: 'module'
	},
	extends: ['vue'],
	plugins: [
		'vue',
		'html'
	],
  rules: {
    'eol-last': 0,
    'indent': 1,
    'no-trailing-spaces': 1,
    'padded-blocks': 0,
    'semi': 0,
    'space-before-function-paren': 0,
    'no-unused-vars': 0
  }
}